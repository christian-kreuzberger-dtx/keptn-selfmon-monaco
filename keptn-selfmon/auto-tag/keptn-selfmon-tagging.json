{
    "name": "{{ .name }}",
    "rules": [
        {
          "type": "SERVICE",
          "enabled": true,
          "valueFormat": "{{ .valueFormat }}",
          "propagationTypes": [
            "SERVICE_TO_PROCESS_GROUP_LIKE"
          ],
          "conditions": [
            {
              "key": {
                "attribute": "PROCESS_GROUP_PREDEFINED_METADATA",
                "dynamicKey": "KUBERNETES_NAMESPACE",
                "type": "PROCESS_PREDEFINED_METADATA_KEY"
              },
              "comparisonInfo": {
                "type": "STRING",
                "operator": "EQUALS",
                "value": "{{ .namespace }}",
                "negate": false,
                "caseSensitive": true
              }
            }
          ]
        }
      ]
}
